@page "/Route-Demo/{Id:int?}"

<h3>RouteDemo</h3>

@for(int i =0; i <= 10; i++)
{
    <p><a href="route-demo/@i?name=micke">Alternativ @i</a></p>
}

<h1>Id är @Id</h1>
<h1>Namnet är @Name</h1>


@code {
    [Parameter]
    public int Id{ get; set;}

    [SupplyParameterFromQuery]
    public string Name { get; set; }


}
